<?xml version="1.0" encoding="UTF-8"?>

<!-- 访问地址中的访问那个webservice的名字 -->
<service name="citicbService">
    <description>中信信用卡</description>

    <parameter name="ServiceObjectSupplier">
        org.apache.axis2.extensions.spring.receivers.SpringServletContextObjectSupplier
    </parameter>

    <!--<parameter name="ServiceClass">-->
        <!--com.shhxzq.bs.service.impl.CiticbServiceImpl-->
    <!--</parameter>-->

    <!-- 配置在applicationContext中配置的bean,这里的值域bean中的id事一样的，否则就报错找不到bean错 -->
    <parameter name="SpringBeanName">citicbService</parameter>

    <messageReceivers>
        <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out" class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
        <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only" class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver" />
    </messageReceivers>
</service>